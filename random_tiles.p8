function place_tiles()
	for x=0,127 do
		for y=0,63 do
			local sp = flr(rnd(5)) + 1  -- random tile id 1ヌ█⧗5
			if rnd(1) < 0.2 then
				mset(x, y, sp)
			else
				mset(x, y, 17)
			end
		end
	end
end

function debug_r_tile()
		for x=0,15 do
		for y=0,15 do
			local t = mget(x,y)
			if t > 0 then
				-- draw the tile number (white, centered-ish)
				print(t, x*8+2, y*8+2, 7)
			end
		end
	end

end

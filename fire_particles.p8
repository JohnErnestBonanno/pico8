pico-8 cartridge // http://www.pico-8.com
version 42
__lua__
function _init()
	
	parts={}
	
	for i=1,10 do
	
		add(parts,{
		x=40+rnd(20),
		y=40+rnd(20),
		r=1,
		c=11,
		speed=1
		})
	end
	
end

function _update()
end

function _draw()
	cls()
	
	for p in all(parts) do 
		circfill(p.x, p.y, p.r, p.c)
	end
	
	
end

function _init()
	x_position = 63
	y_position = 63
	button = "" 
end

function _update()
	
	if btn(⬆️) then
		y_position-=1
	end
	
	if btn(⬇️) then 
		y_position+=1
	end
end

function _draw()
	cls() -- clear screen
	spr(1,x_position,y_position)
	print(button)

end



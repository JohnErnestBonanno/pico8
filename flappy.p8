pico-8 cartridge // http://www.pico-8.com
version 42
__lua__
-- top right is 0,0
-- x gets bigger to right
-- y gets bigger to bottom 

function _init()
-- called when prog runs

	px=63
	py=63

	flap = 0
	flap_amt=8
	gravity =2
	
	--sub function
	i_pipes()
	--set up pipes

end

function _update()
--called 30x a sec

	py=py+gravity --gravity
	if btnp(❎) then 
		flap = 20 
		end
		
	if flap>0 then
		flap=flap-5
	end

	py=py-flap
	
	if py>=120 then 
		py=120
	elseif py<=0 then
		py=0
	end
	
	u_pipes()
	

end

function _draw()
	--called 30x a sec after update
	cls()--clear screen
	map()--draw map
	
	d_pipes()
	
	spr(1,px,py)
	--sprite num, x, y 

end



-->8
--pipes--

function i_pipes()
--set up pipes
	p1x=120--pipe1-x
	p1y=63--pipe1-y
end

function u_pipes()
--update pipes
	p1x-=1

	--loop pipe
	if p1x <-16 then 
		p1x=128
		end
end
	
function d_pipes()
--draw pipes
	spr(7,p1x,p1y,2,2)
end

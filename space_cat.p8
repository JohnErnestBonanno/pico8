version 42
__lua__
function _init()
	
-- player class -- 
	player={
		x=63,
		y=63,
		flip_y = false,
		sp=1
	} 
end

function _update()

-- x movement --
	if btn(➡️) then
		player.x+=1
		player.flip_y=false
		player.flip_x=false
		player.sp=1
	end
	
	if btn(⬅️) then 
		player.x-=1
		player.flip_y=true
		player.flip_x=false
		player.sp=1
	end

-- y movement -- 
	if btn(⬆️) then
		player.y-=1
		player.sp=2
		player.flip_x=false
	end
	
	if btn(⬇️) then 
		player.y+=1
		player.sp=2
		player.flip_x=true
	end
end

function _draw()
	cls(3) -- clear screen into green
	map()
	spr(
		player.sp, -- which sprite
		player.x, -- x coor
		player.y, -- y coor
		1, -- width
		1, -- height
		player.flip_y,
		player.flip_x
		)
		
	print(mget(flr((player.x+4)/8),flr((player.y+4)/8)))
	-- /8 because each sprite is 8x8 pixels
	-- takes top left by default

end
-->8

-->8
